# Development environment configuration for arcanum
# Copy this file to .env.local and modify as needed

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/arcanum
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=arcanum
DATABASE_USER=postgres
DATABASE_PASSWORD=postgres

# Development Settings
DEBUG=true
LOG_LEVEL=DEBUG

# pgAdmin Configuration (when using --profile admin)
PGADMIN_DEFAULT_EMAIL=admin@arcanum.local
PGADMIN_DEFAULT_PASSWORD=admin_password